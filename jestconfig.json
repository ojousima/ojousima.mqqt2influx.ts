{
  "mapCoverage": true,
  "moduleFileExtensions": ["js", "ts"],
  "testResultsProcessor": "jest-sonar-reporter",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(../tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}